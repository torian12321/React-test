{
  "name": "boilerplate",
  "author": "Aitor Palomares",
  "description": "Simple react app for learning",
  "license": "MIT",
  "version": "0.1.0",
  "main": "export.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/torian12321/React-test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --hot --host 0.0.0.0",
    "test": "jest --watch",
    "jest": "jest",
    "build": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production  webpack --progress -p",
    "build:dist": "npm run build:prod",
    "check-updates": "node _config/updatePackages",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "styleguide": "cross-env NODE_ENV=development styleguidist server",
    "styleguide:build": "cross-env NODE_ENV=production  styleguidist build"
  },
  "dependencies": {
    "loaders": "1.1.3",
    "prop-types": "15.6.0",
    "preact": "8.2.5",
    "preact-compat": "3.17.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-classnames": "0.1.2",
    "uniquid": "1.1.4",
    "react-styleguidist": "6.0.26",
    "react-icons": "2.2.5",
    "less-loader": "4.0.5",
    "less": "2.7.2",
    "css-loader": "0.28.7",
    "style-loader": "0.18.2",
    "react-css-modules": "4.7.1"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "ejs": "2.5.7",
    "ejs-html-loader": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "20.0.4",
    "jest-css-modules": "^1.1.0",
    "npm-check-updates": "2.12.1",
    "react-test-renderer": "15.4.2",
    "uniqid": "4.1.1",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-dashboard": "1.0.0-5",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "4.1.0",
    "webpack-preset": "0.2.0",
    "webpack-preset-babel": "0.2.0",
    "webpack-preset-babel-es2015": "0.2.0"
  },
  "jest": {
    "automock": false,
    "notify": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/file_mock.js",
      "\\.(mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/audio_mock.js",
      "\\.(css|less|scss)$": "<rootDir>/test/style_mock.js"
    },
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules"
  }
}
